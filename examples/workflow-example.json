      "type": "n8n-nodes-base.manualTrigger",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "resource": "database",
        "operation": "createDocument",
        "databaseId": "main",
        "collectionId": "users",
        "documentId": "unique()",
