      "position": [450, 300],
      "credentials": {
        "appwriteApi": {
          "id": "1",
          "name": "Appwrite API"
        }
      }
    },
    {
      "parameters": {
        "resource": "database",
        "operation": "listDocuments",
        "databaseId": "main",
        "collectionId": "users"
      },
      "id": "appwrite-list-docs",
      "name": "List All Users",
      "type": "n8n-nodes-appwrite-full.appwrite",
      "typeVersion": 1,
      "position": [650, 300],
      "credentials": {
        "appwriteApi": {
          "id": "1",
          "name": "Appwrite API"
        }
      }
    }
  ],
  "connections": {
    "When clicking 'Test workflow'": {
      "main": [
        [
          {
            "node": "Create User Document",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Create User Document": {
      "main": [
        [
          {
            "node": "List All Users",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  }
