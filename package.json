{
  "name": "n8n-nodes-appwrite-full",
  "version": "0.10.1",
  "description": "n8n community node for Appwrite - Complete integration with all Appwrite services",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "appwrite",
    "backend",
    "baas",
    "database",
    "storage",
    "authentication"
  ],
  "license": "MIT",
  "homepage": "https://github.com/timiliris/n8n-nodes-appwrite",
  "author": {
    "name": "Timothy Iliris",
    "email": "timiliris@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timiliris/n8n-nodes-appwrite.git"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp nodes/Appwrite/AppwriteLogo.svg dist/nodes/Appwrite/ && cp nodes/Appwrite/AppwriteLogo.svg dist/nodes/AppwriteFormTrigger/ && cp nodes/Appwrite/AppwriteLogo.svg dist/nodes/AppwriteHelper/",
    "dev": "npm run build && n8n start",
    "lint": "eslint nodes credentials --ext .ts",
    "lint:fix": "eslint nodes credentials --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "prepare": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AppwriteApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Appwrite/Appwrite.node.js",
      "dist/nodes/AppwriteHelper/AppwriteHelper.node.js",
      "dist/nodes/AppwriteFormTrigger/AppwriteFormTrigger.node.js"
    ]
  },
  "dependencies": {
    "node-appwrite": "^20.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
